.donate-modal .modal-content {
  border-radius: 0px;
  background-clip: border-box;
  -webkit-box-shadow: none;
  box-shadow: none;
  border: none;
  height:auto;
  padding: 40px;
  text-align: center;
}
.donate-modal .modal-content h2 {
  margin-bottom: 15px;
  font-size: 1em;
}
.donate-modal .modal-content p {
  margin-bottom: 30px;
}
.donate-modal .modal-content p.item-intro {
  margin: 20px 0 30px;
  font-family: "Droid Serif", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-style: italic;
  font-size: 16px;
}
.donate-modal .modal-content ul.list-inline {
  margin-bottom: 30px;
  margin-top: 0;
}
.donate-modal .modal-content img {
  margin-bottom: 30px;
}
.donate-modal .close-modal {
  position: absolute;
  width: 75px;
  height: 75px;
  background-color: transparent;
  top: 25px;
  right: 25px;
  cursor: pointer;
}
.donate-modal .close-modal:hover {
  opacity: 0.3;
}
.donate-modal .modal-backdrop {
  opacity: 0;
  display: none;
  width:80%;
}
